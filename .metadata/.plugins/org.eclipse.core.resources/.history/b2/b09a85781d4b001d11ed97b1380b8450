package ArrayStringAssingment13oct;

public class LcmAndHcf {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int a[] = {12,16};
		
		for (int i = 0; i < a.length-1; i++) 
		{
			int j=1;
			if(a[i]%j==0 && a[i+1]%j==0)
			{
				System.out.println(i);
			}
			j++;
		}

	}

}
