package Practise;

public class VowelInString {

	public static void main(String[] args) {

         String str= "prasad birari";
          int i=0; 
        	 
        	 if(str.charAt(i)=='a' || str.charAt(i)=='e' || str.charAt(i)=='i' || str.charAt(i)=='o' || str.charAt(i)=='u')
        			 {
        		        //count++;
        		        System.out.println(str.charAt(i)+" "+i );  
        			 }
			
        	 
		}
	}

}
