package Practise;

public class ReverseStringCharArray {

	public static void main(String[] args) {

		String str = "done this code ";
		
		String sa[] =str.split(" ");
		
		char ch[] = str.toCharArray();
		
		int i=0;
		int j=ch.length-1;
		
			while(i>j)
			{
				char temp = ch[i];
				     ch[i]=ch[j];
				     ch[j]=ch[i];
				     i++;
				     j--;
			}
			str = new String(ch);
         
        	 System.out.println(str+" ");
	
	}

}
