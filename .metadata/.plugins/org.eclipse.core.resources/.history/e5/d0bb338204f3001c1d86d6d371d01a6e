package java8Assingment;

import java.util.Arrays;
import java.util.List;
import java.util.stream.Collectors;

public class DistinctMarksAss2 {
	
	public static void main(String[] args) {
		
		List<Integer> marks = Arrays.asList(9, 10, 3, 4, 7, 3, 4);
		List<Integer> distinct = marks.stream().map( i -> i*i).distinct().collect(Collectors.toList());
		System.out.println(distinct);


	}
}
//WAP to implement predicate to check given no is even or not
