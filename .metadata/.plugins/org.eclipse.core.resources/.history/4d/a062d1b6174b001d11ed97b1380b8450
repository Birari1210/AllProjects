package ArrayStringAssingment13oct;

public class Akash {

	public static void main(String[] args)
	{
		int num=496;
		int sum=0;
		for(int i=1;i<num;i++)
		{
			if(num%i==0)
			{
				sum= sum + i;
			}
		}
		if(sum==num)
		{
			System.out.println("perfect number");
		}
		else
		{
			System.out.println("not ");
		}
		System.out.println(sum);
	}

}
