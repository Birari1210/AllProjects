package ArrayStringAssingment13oct;

public class Akash {

	public static void main(String[] args)
	{
		int num=28;
		int a[] = {2,3,4,5,6,7,9,10};
		int sum=0;
		for(int i=1;i<a.length;i++)
		{
			if(a[i]%i==0)
			{
				sum= sum + i;
			}
		}
		if(sum==num)
		{
			System.out.println("perfect number");
		}
		else
		{
			System.out.println("not ");
		}
		
	}

}
