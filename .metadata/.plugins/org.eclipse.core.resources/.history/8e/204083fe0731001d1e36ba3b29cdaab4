package Practise;

public class ReverseStringCharArray {

	public static void main(String[] args) {

		String str = "done this code ";
		
		String sa[] =str.split(" ");
		
		char ch[] = str.toCharArray();
		
		String s ="";
		int i=0;
		int j=ch.length-1;
		
			while(i<j)
			{
				char temp = ch[i];
				     ch[i]=ch[j];
				     ch[j]=temp;
				     i++;
				     j--;
			
            s=s+str;
			}
        	 System.out.println(s+" ");
	
	}

}
